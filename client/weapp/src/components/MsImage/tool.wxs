/**
 * ms-image WXS 工具函数
 * 处理图片 URL：空值返回占位图，加载出错返回错误图
 */
function handleUrl(src, error, placeholderUrl, errorUrl) {
  if (!src || src === '') {
    return placeholderUrl || ''
  }
  if (error) {
    return errorUrl || placeholderUrl || ''
  }
  return src
}

module.exports = {
  handleUrl: handleUrl,
}
